<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE decorators>
<decorators defaultdir="/WEB-INF/jsp/layout">
	<excludes>
		<pattern>/popup/*</pattern>
	</excludes>

	<decorator name="default" page="defaultLayout.jsp">
		<pattern>/*</pattern>
	</decorator>
	
	<decorator name="acl" page="aclLayout.jsp">
		<pattern>/acl/*</pattern>
	</decorator>
	
	<decorator name="sample" page="sampleLayout.jsp">
		<pattern>/sample*</pattern>
	</decorator>
	
	

</decorators>